---
import Screen from "../layouts/Screen.astro";
---

<Screen title="Reset Phrase Info" pageName="resetPhraseInfo">
  <script>
    import { toast } from "$src/components/toast";
    import { i18n } from "../i18n";
    import { resetPhraseInfoPage } from "$src/flows/manage/deviceSettings";

    resetPhraseInfoPage({
      next: () => toast.info("next"),
      cancel: () => toast.info("cancel"),
      i18n,
    });
  </script>
</Screen>
