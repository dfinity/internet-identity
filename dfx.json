{
  "canisters": {
    "idp_service": {
      "type": "custom",
      "candid": "src/idp_service/idp_service.did",
      "wasm": "target/wasm32-unknown-unknown/release/idp_service.wasm",
      "build": "cargo build --release --target wasm32-unknown-unknown"
    },
    "frontend": {
      "dependencies": ["idp_service"],
      "frontend": {
        "entrypoint": "src/frontend/src/index.ts"
      },
      "source": ["src/frontend/assets", "dist"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.0-beta.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
