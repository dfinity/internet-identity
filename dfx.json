{
  "canisters": {
    "idp_service": {
      "type": "custom",
      "candid": "src/idp_service/idp_service.did",
      "wasm": "target/wasm32-unknown-unknown/release/idp_service.wasm",
      "build": "src/idp_service/build.sh"
    },
    "frontend": {
      "dependencies": ["idp_service"],
      "frontend": {
        "entrypoint": "src/frontend/src/index.ts"
      },
      "source": ["src/frontend/assets", "dist"],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
