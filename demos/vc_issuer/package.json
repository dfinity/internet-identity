{
  "name": "vc_issuer",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@dfinity/auth-client": "^0.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.2.1"
  },
  "scripts": {
    "dev": "vite --config ./vite.config.ts",
    "build": "vite --config ./vite.config.ts build",
    "vite": "vite",
    "generate": "npm run generate:types && npm run generate:js",
    "generate:types": "didc bind ./vc_issuer.did -t ts > generated/vc_issuer_types.d.ts",
    "generate:js": "didc bind ./vc_issuer.did -t js > ./generated/vc_issuer_idl.js"
  },
  "devDependencies": {
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0"
  }
}
