{
  "name": "@dfinity/internet-identity",
  "version": "0.1.0",
  "type": "module",
  "description": "",
  "keywords": [],
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "dev": "II_FETCH_ROOT_KEY=1 II_DUMMY_CAPTCHA=1 vite",
    "host": "II_FETCH_ROOT_KEY=1 II_DUMMY_CAPTCHA=1 vite --host",
    "showcase": "astro dev --root ./src/showcase",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "tsc --project ./tsconfig.all.json --noEmit",
    "check:showcase": "astro check --root ./src/showcase --tsconfig ../../tsconfig.all.json",
    "watch": "npm run check -- --watch",
    "opts": "TS_NODE_PROJECT=tsconfig.base.json NODE_OPTIONS='--loader ts-node/esm --experimental-specifier-resolution=node' \"$@\"",
    "generate": "npm run generate:types && npm run generate:js && npm run generate:types-issuer && npm run generate:js-issuer",
    "generate:types": "didc bind ./src/internet_identity/internet_identity.did -t ts > src/frontend/src/lib/generated/internet_identity_types.d.ts",
    "generate:js": "didc bind ./src/internet_identity/internet_identity.did -t js > src/frontend/src/lib/generated/internet_identity_idl.js",
    "generate:types-issuer": "didc bind ./demos/vc_issuer/vc_demo_issuer.did -t ts > src/vc-api/src/generated/vc_issuer_types.ts",
    "generate:js-issuer": "didc bind ./demos/vc_issuer/vc_demo_issuer.did -t js > src/frontend/src/lib/generated/vc_issuer_idl.js",
    "build:showcase": "npm run check:showcase && astro build --root ./src/showcase",
    "preview:showcase": "astro preview --root ./src/showcase",
    "screenshots": "npm run opts -- ./src/frontend/screenshots.ts",
    "test": "vitest --config ./vitest.config.ts",
    "test:e2e": "VITEST_MAX_THREADS=1 vitest --mode e2e --config ./vitest.config.ts",
    "test:e2e-desktop": "SCREEN=desktop npm run test:e2e",
    "test:e2e-mobile": "SCREEN=mobile npm run test:e2e",
    "test:e2e-playwright": "npx playwright test",
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint --max-warnings 0 --cache --cache-location node_modules/.cache/eslint 'src/frontend/**/*.ts*' 'src/showcase/**/*.ts'",
    "lint:lit": "lit-analyzer -- --rules.no-incompatible-type-binding off --rules.no-invalid-directive-binding off --rules.no-noncallable-event-binding off ./src/showcase ./src/frontend",
    "format": "prettier --write src/showcase src/frontend src/vc-api src/sig-verifier-js src/vite-plugins tsconfig.json tsconfig.all.json eslint.config.js vite.config.ts vitest.config.ts demos",
    "format-check": "prettier --check src/showcase src/frontend src/vc-api src/sig-verifier-js src/vite-plugins tsconfig.json tsconfig.all.json eslint.config.js vite.config.ts vitest.config.ts demos"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/svelte": "^7.0.9",
    "@dfinity/internet-identity-vite-plugins": "*",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.3",
    "@types/dompurify": "^3.0.5",
    "@types/node": "*",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitest/browser": "^3.0.9",
    "@wdio/globals": "^8.43.0",
    "@zag-js/svelte": "^1.12.0",
    "@zag-js/toast": "^1.12.0",
    "astro": "^5.5.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "fake-indexeddb": "^4.0.2",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "lit-analyzer": "^2.0.2",
    "prettier": "3.5.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.20.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.1.3",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.4",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.0.9",
    "webdriverio": "^8.43.0"
  },
  "dependencies": {
    "@dfinity/agent": "^2.1.2",
    "@dfinity/auth-client": "^2.1.2",
    "@dfinity/candid": "^2.1.2",
    "@dfinity/identity": "^2.1.2",
    "@dfinity/internet-identity-vc-api": "*",
    "@dfinity/principal": "^2.1.2",
    "@dfinity/utils": "^2.5.1",
    "@lucide/svelte": "^0.508.0",
    "@noble/hashes": "^1.3.1",
    "bip39": "^3.0.4",
    "borc": "^2.1.1",
    "buffer": "^6.0.3",
    "dompurify": "^3.2.4",
    "idb-keyval": "^6.2.1",
    "lit-html": "^3.2.1",
    "marked": "^11.0.0",
    "plausible-tracker": "0.3.9",
    "qr-creator": "^1.0.0",
    "stream-browserify": "^3.0.0",
    "ua-parser-js": "^1.0.35",
    "zod": "^3.22.3"
  },
  "overrides": {
    "ws": "8.17.1"
  },
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "workspaces": [
    "src/vc-api",
    "demos/test-app",
    "demos/vc_issuer",
    "src/vite-plugins",
    "src/sig-verifier-js"
  ]
}
